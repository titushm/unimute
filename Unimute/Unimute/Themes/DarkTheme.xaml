<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!-- Global Colours -->
	<SolidColorBrush x:Key="Window.Primary.Background" Color="#25282c"/>
	<SolidColorBrush x:Key="Text.Foreground" Color="#e1e1e1"/>
	<SolidColorBrush x:Key="Text.Secondary.Foreground" Color="#A0A0A0"/>

	<!-- Button Colours -->
	<SolidColorBrush x:Key="Button.Background" Color="#3e434b"/>
	<SolidColorBrush x:Key="Button.Foreground" Color="#ffffff"/>
	<SolidColorBrush x:Key="Button.Border" Color="#59626e"/>
	<Thickness x:Key="Button.BorderThickness">1</Thickness>
	<Thickness x:Key="Button.Padding">10,5,10,5</Thickness>
	<FontFamily x:Key="Button.FontFamily">Segoe fUI</FontFamily>
	<FontWeight x:Key="Button.FontWeight">Normal</FontWeight>
	<x:Double x:Key="Button.FontSize">14</x:Double>
	<x:Boolean x:Key="Button.UseSystemFocusVisuals">True</x:Boolean>
	<Thickness x:Key="Button.FocusVisualMargin">-3</Thickness>

	<SolidColorBrush x:Key="Button.Background.PointerOver" Color="#484e58"/>
	<SolidColorBrush x:Key="Button.Border.PointerOver" Color="#59626e"/>
	<SolidColorBrush x:Key="Button.Foreground.PointerOver" Color="#ffffff"/>

	<SolidColorBrush x:Key="Button.Background.Pressed" Color="#444a53"/>
	<SolidColorBrush x:Key="Button.Border.Pressed" Color="#59626e"/>
	<SolidColorBrush x:Key="Button.Foreground.Pressed" Color="#ffffff"/>

	<SolidColorBrush x:Key="Button.Background.Disabled" Color="#2a2a2a"/>
	<SolidColorBrush x:Key="Button.Border.Disabled" Color="#555555"/>
	<SolidColorBrush x:Key="Button.Foreground.Disabled" Color="#888888"/>
	
	<!-- Invert Button Colours -->
	<SolidColorBrush x:Key="Button.Invert.Background" Color="#e1e1e1"/>
	<SolidColorBrush x:Key="Button.Invert.Foreground" Color="#3a3a3a"/>
	<SolidColorBrush x:Key="Button.Invert.Border" Color="#3a3a3a"/>
	<Thickness x:Key="Button.Invert.BorderThickness">2</Thickness>
	<Thickness x:Key="Button.Invert.Padding">10,5,10,5</Thickness>
	<FontFamily x:Key="Button.Invert.FontFamily">Segoe UI</FontFamily>
	<FontWeight x:Key="Button.Invert.FontWeight">Normal</FontWeight>
	<x:Double x:Key="Button.Invert.FontSize">14</x:Double>
	<x:Boolean x:Key="Button.Invert.UseSystemFocusVisuals">True</x:Boolean>
	<Thickness x:Key="Button.Invert.FocusVisualMargin">-3</Thickness>

	<SolidColorBrush x:Key="Button.Invert.Background.PointerOver" Color="#d4d4d4"/>
	<SolidColorBrush x:Key="Button.Invert.Border.PointerOver" Color="Transparent"/>
	<SolidColorBrush x:Key="Button.Invert.Foreground.PointerOver" Color="#000000"/>

	<SolidColorBrush x:Key="Button.Invert.Background.Pressed" Color="#e1e1e1"/>
	<SolidColorBrush x:Key="Button.Invert.Border.Pressed" Color="Transparent"/>
	<SolidColorBrush x:Key="Button.Invert.Foreground.Pressed" Color="#000000"/>

	<SolidColorBrush x:Key="Button.Invert.Background.Disabled" Color="#888888"/>
	<SolidColorBrush x:Key="Button.Invert.Border.Disabled" Color="Transparent"/>
	<SolidColorBrush x:Key="Button.Invert.Foreground.Disabled" Color="#aaaaaa"/>

	<!-- Warning Button Colours -->
	<SolidColorBrush x:Key="Button.Warning.Background" Color="#FFC107"/>
	<SolidColorBrush x:Key="Button.Warning.Foreground" Color="#000000"/>
	<SolidColorBrush x:Key="Button.Warning.Border" Color="#FF9800"/>
	<Thickness x:Key="Button.Warning.BorderThickness">2</Thickness>
	<Thickness x:Key="Button.Warning.Padding">5,5,5,5</Thickness>
	<FontFamily x:Key="Button.Warning.FontFamily">Segoe UI</FontFamily>
	<FontWeight x:Key="Button.Warning.FontWeight">SemiBold</FontWeight>
	<x:Double x:Key="Button.Warning.FontSize">14</x:Double>
	<x:Boolean x:Key="Button.Warning.UseSystemFocusVisuals">True</x:Boolean>
	<Thickness x:Key="Button.Warning.FocusVisualMargin">-3</Thickness>

	<SolidColorBrush x:Key="Button.Warning.Background.PointerOver" Color="#FFA000"/>
	<SolidColorBrush x:Key="Button.Warning.Border.PointerOver" Color="#FFD54F"/>
	<SolidColorBrush x:Key="Button.Warning.Foreground.PointerOver" Color="#000000"/>

	<SolidColorBrush x:Key="Button.Warning.Background.Pressed" Color="#FFA000"/>
	<SolidColorBrush x:Key="Button.Warning.Border.Pressed" Color="#FFD54F"/>
	<SolidColorBrush x:Key="Button.Warning.Foreground.Pressed" Color="#000000"/>

	<SolidColorBrush x:Key="Button.Warning.Background.Disabled" Color="#FFE0B2"/>
	<SolidColorBrush x:Key="Button.Warning.Border.Disabled" Color="#FFCC80"/>
	<SolidColorBrush x:Key="Button.Warning.Foreground.Disabled" Color="#9E9E9E"/>
	
	<!-- Success Button Colours -->
	<SolidColorBrush x:Key="Button.Success.Background" Color="#66BB6A"/>
	<SolidColorBrush x:Key="Button.Success.Foreground" Color="#000000"/>
	<SolidColorBrush x:Key="Button.Success.Border" Color="#388E3C"/>
	<Thickness x:Key="Button.Success.BorderThickness">2</Thickness>
	<Thickness x:Key="Button.Success.Padding">10,5,10,5</Thickness>
	<FontFamily x:Key="Button.Success.FontFamily">Segoe UI</FontFamily>
	<FontWeight x:Key="Button.Success.FontWeight">Normal</FontWeight>
	<x:Double x:Key="Button.Success.FontSize">14</x:Double>
	<x:Boolean x:Key="Button.Success.UseSystemFocusVisuals">True</x:Boolean>
	<Thickness x:Key="Button.Success.FocusVisualMargin">-3</Thickness>

	<SolidColorBrush x:Key="Button.Success.Background.PointerOver" Color="#81C784"/>
	<SolidColorBrush x:Key="Button.Success.Border.PointerOver" Color="Transparent"/>
	<SolidColorBrush x:Key="Button.Success.Foreground.PointerOver" Color="#000000"/>

	<SolidColorBrush x:Key="Button.Success.Background.Pressed" Color="#81C784"/>
	<SolidColorBrush x:Key="Button.Success.Border.Pressed" Color="Transparent"/>
	<SolidColorBrush x:Key="Button.Success.Foreground.Pressed" Color="#000000"/>

	<SolidColorBrush x:Key="Button.Success.Background.Disabled" Color="#388E3C"/>
	<SolidColorBrush x:Key="Button.Success.Border.Disabled" Color="Transparent"/>
	<SolidColorBrush x:Key="Button.Success.Foreground.Disabled" Color="#B0BEC5"/>
</ResourceDictionary>
